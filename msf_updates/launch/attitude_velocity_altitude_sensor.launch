<!--launch file-->
<launch>
    <node  launch-prefix="gdb -ex run --args" name="msf_pose_velocity_altitude_sensor" pkg="msf_updates" type="pose_velocity_altitude_sensor" clear_params="true" output="screen">
            <!--<remap from="msf_core/hl_state_input" to="/bluebird/fcu/ekf_state_out" />-->
            <remap from="msf_updates/pose_input" to="/drone6/" />
					  <remap from="msf_updates/attitude" to="/drone6/correct_imu_data" />
					 	<remap from="/velocity" to="/velocity" />
					 	<remap from="/msf_updates/altitude_above_takeoff" to="/altitude_above_takeoff" />	    		    	
						<remap from="msf_core/imu_state_input" to="drone6/dji_sdk/imu" />	    	

            <rosparam file="$(find msf_updates)/pose_pressure_sensor_fix.yaml"/>
    </node>
</launch>
